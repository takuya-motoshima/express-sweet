            <td><code>replace()</code></td>
            <td>
              Returns a new string with some or all matches of a pattern replaced by a replacement.
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Parameters:</h5>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>val: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">String.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>find: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">The string to be replaced.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>replace: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">The string to replace.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Return:</h5>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string</code> Character string after replacement.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Code-->
              <div class="pt-3">
                <!--begin::Highlight-->
                <div class="highlight">
                  <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                  <div class="highlight-code">
                    <pre class="language-javascript"><code class="language-javascript">\{{replace &#039;The quick brown fox jumps over the lazy dog. If the dog reacted, was it really lazy?&#039; &#039;dog&#039; &#039;monkey&#039;}}</code></pre>
                  </div>
                </div>
                <!--end::Highlight-->
              </div>
              <!--end::Code-->
            </td>
          </tr>
          <tr>
            <td><code>split()</code></td>
            <td>
              Split `string` by the given `character`.
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Parameters:</h5>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>val: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">String.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>separator: string</code>
                  <!--begin::Block-->
                  <div class="pt-3">A character that delimits the substrings in this string. Default is a comma.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Return:</h5>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string[]</code> An Array of strings, split at each point where the separator occurs in the given string. The default is a comma.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Code-->
              <div class="pt-3">
                <!--begin::Highlight-->
                <div class="highlight">
                  <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                  <div class="highlight-code">
                    <pre class="language-javascript"><code class="language-javascript">\{{!-- results in: [&#039;a&#039;, &#039;b&#039;, &#039;c&#039;] --}}
\{{split &quot;a,b,c&quot; &quot;,&quot;}}

\{{!-- results in: &lt;div&gt;a&lt;/div&gt;&lt;div&gt;b&lt;/div&gt;&lt;div&gt;c&lt;/div&gt; --}}
\{{#each (split list &#039;,&#039;)}}
  &lt;div&gt;\{{this}}&lt;/div&gt;
\{{/each}}</code></pre>
                  </div>
                </div>
                <!--end::Highlight-->
              </div>
              <!--end::Code-->
            </td>
          </tr>
          <tr>
            <td><code>formatBytes()</code></td>
            <td>
              Convert bytes to just the right units(KB, MB, GB, TB, PB, EB, ZB, YB).
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Parameters:</h5>
              <!--end::Heading-->
              <!--begin::Parameters-->
              <ul>
                <li class="py-2"><code>bytes: number</code>
                  <!--begin::Block-->
                  <div class="pt-3">Bytes.</div>
                  <!--end::Block-->
                </li>
                <li class="py-2"><code>decimals?: number</code>
                  <!--begin::Block-->
                  <div class="pt-3">Number of decimal places to display. Default is 0.</div>
                  <!--end::Block-->
                </li>
              </ul>
              <!--end::Parameters-->
              <!--begin::Heading-->
              <h5 class="py-2 mb-0">Return:</h5>
              <!--end::Heading-->
              <!--begin::Return-->
              <ul class="list-style-none">
                <li class="py-2"><code>string</code> Returns a value with units.</li>
              </ul>
              <!--end::Return-->
              <!--begin::Code-->
              <div class="pt-3">
                <!--begin::Highlight-->
                <div class="highlight">
                  <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                  <div class="highlight-code">
                    <pre class="language-javascript"><code class="language-javascript">\{{!-- results in: 1 KB --}}
\{{formatBytes 1024}}

\{{!-- results in: 1.21 KB --}}
\{{formatBytes 1234 2}}

\{{!-- results in: 1.205 KB --}}
\{{formatBytes 1234 3}}

\{{!-- results in: 0 Bytes --}}
\{{formatBytes 0}}</code></pre>
                  </div>
                </div>
                <!--end::Highlight-->
              </div>
              <!--end::Code-->
            </td>
          </tr>
        </tbody>
        <!--end::Body-->
      </table>
      <!--end::Table-->
    </div>
    <!--end::Table wrapper-->
  </div>
  <!--end::Block-->
</div>
