        <tr>
          <td><code>public deleteCollection()</code></td>
          <td>
            Deletes the specified collection.<br>
            Note that this operation removes all faces in the collection.
            <!--begin::Heading-->
            <h5 class="py-3 mb-0">Parameters:</h5>
            <!--end::Heading-->
            <!--begin::Parameters-->
            <ul>
              <li class="py-2"><code>collectionId: string</code>
                <!--begin::Block-->
                <div class="pt-3">ID of the collection to delete.</div>
                <!--end::Block-->
              </li>
            </ul>
            <!--end::Parameters-->
            <!--begin::Heading-->
            <h5 class="py-3 mb-0">Return:</h5>
            <!--end::Heading-->
            <!--begin::Return-->
            <ul class="list-style-none">
              <li class="py-2"><code>Promise&lt;boolean&gt;</code> True on success.</li>
            </ul>
            <!--end::Return-->
            <!--begin::Code-->
            <div class="pt-3">
              <!--begin::Highlight-->
