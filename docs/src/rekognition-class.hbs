        <tr>
          <td><code>public compareFaces()</code></td>
          <td>
            Compare the similarity of two faces.
            <!--begin::Heading-->
            <h5 class="py-3 mb-0">Parameters:</h5>
            <!--end::Heading-->
            <!--begin::Parameters-->
            <ul>
              <li class="py-2"><code>img1: string</code>
                <!--begin::Block-->
                <div class="pt-3">Image path or Data Url or image buffer.</div>
                <!--end::Block-->
              </li>
              <li class="py-2"><code>img2: string</code>
                <!--begin::Block-->
                <div class="pt-3">Image path or Data Url or image buffer.</div>
                <!--end::Block-->
              </li>
            </ul>
            <!--end::Parameters-->
            <!--begin::Heading-->
            <h5 class="py-3 mb-0">Return:</h5>
            <!--end::Heading-->
            <!--begin::Return-->
            <ul class="list-style-none">
              <li class="py-2"><code>Promise&lt;number&gt;</code> Level of confidence that the faces match.</li>
            </ul>
            <!--end::Return-->
            <!--begin::Code-->
            <div class="pt-3">
              <!--begin::Highlight-->
              <div class="highlight">
                <button class="highlight-copy btn" data-bs-toggle="tooltip" title="Copy code">copy</button>
                <ul class="nav nav-pills" role="tablist">
                  <li class="nav-item">
                    <a class="nav-link active" data-bs-toggle="tab" href="#compare-faces-method-esm-tab" role="tab">ESM</a>
                  </li>
                  <li class="nav-item">
                    <a class="nav-link" data-bs-toggle="tab" href="#compare-faces-method-cjs-tab" role="tab">CJS</a>
                  </li>
                </ul>
                <div class="tab-content">
                  <div class="tab-pane fade show active" id="compare-faces-method-esm-tab" role="tabpanel">
                    <div class="highlight-code">
                      <pre class="language-javascript"><code class="language-javascript">import * as sweet from &#039;express-sweet&#039;;
import fs from &#039;fs&#039;;

// Load environment variables.
sweet.middlewares.Environment.mount();

// Rekognition Client.
const client = new sweet.services.AWSRekognitionClient({
  region: process.env.AWS_REKOGNITION_REGION,
  accessKeyId: process.env.AWS_REKOGNITION_ACCESS_KEY_ID,
  secretAccessKey: process.env.AWS_REKOGNITION_SECRET_ACCESS_KEY,
});

// Compare faces from path.
await client.compareFaces(&#039;img1.jpg&#039;, &#039;img2.jpg&#039;);

// Compare faces from data URL.
await client.compareFaces(&#039;data:image/png;base64,/9j/4...&#039;, &#039;data:image/png;base64,/9j/4...&#039;);

// Compare faces from buffer.
await client.compareFaces(fs.readFileSync(&#039;img1.jpg&#039;), fs.readFileSync(&#039;img1.jpg&#039;));</code></pre>
                    </div>
                  </div>
                  <div class="tab-pane fade" id="compare-faces-method-cjs-tab" role="tabpanel">
